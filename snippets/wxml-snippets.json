  "TDesign DropdownMenu:optionsColumns 下拉菜单": {
    "prefix": "td-dropdown-menu:optionsColumns",
    "body": [
      "<t-dropdown-menu>",
      "  <t-dropdown-item",
      "    label=\"单列多选\"",
      "    options=\"{{multipleSelect.options}}\"",
      "    value=\"{{multipleSelect.value}}\"",
      "    bindchange=\"handleMultipleSelect\"",
      "    multiple",
      "  />",
      "  <t-dropdown-item",
      "    label=\"双列多选\"",
      "    optionsColumns=\"2\"",
