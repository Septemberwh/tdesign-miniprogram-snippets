      "    defaultValue=\"{{['option_1', 'option_2']}}\"",
      "    multiple",
      "  />",
      "  <t-dropdown-item",
      "    label=\"三列多选\"",
      "    optionsColumns=\"3\"",
      "    options=\"{{tripleColumnsOptions}}\"",
      "    defaultValue=\"{{['option_1', 'option_2', 'option_3']}}\"",
      "    multiple",
      "  />",
      "</t-dropdown-menu>"
    ],
    "description": "TDesign DropdownMenu 组件"
  },
