{
  "TMS: view": {
    "prefix": "view",
    "body": [
      "<view class=\"$1\">$2</view>"
    ],
    "description": "WXML: view"
  },
  "TMS: text": {
    "prefix": "text",
    "body": [
      "<text class=\"$1\">$2</text>"
    ],
    "description": "WXML: text"
  },
  "TMS: block": {
    "prefix": "block",
    "body": [
      "<block>$1</block>"
    ],
    "description": "WXML: block"
  },
  "TMS: image": {
    "prefix": "image",
    "body": [
      "<image class=\"\" src=\"$1\" mode=\"widthFix\"></image>"
    ],
    "description": "WXML: image"
  },
  "TMS: template": {
    "prefix": "wxml-template",
    "body": [
      "<template name=\"msgItem\">",
      "  <view>",
      "    <text> {{index}}: {{msg}} </text>",
      "    <text> Time: {{time}} </text>",
      "  </view>",
      "</template>"
    ],
    "description": "Reference: WXML 语法：模板"
  },
  "TMS: template:is": {
    "prefix": "wxml-template:is",
    "body": [
      "<template is=\"msgItem\" data=\"{{...item}}\"/>"
    ],
    "description": "Reference: WXML 语法 template"
  },
  "TMS: template:import": {
    "prefix": "wxml-template:import",
    "body": [
      "<import src=\"item.wxml\"/>"
    ],
    "description": "Reference: WXML 语法：引入"
  },
  "TMS: include": {
    "prefix": "wxml-include",
    "body": [
      "<include src=\"header.wxml\"/>"
    ],
    "description": "Reference: WXML 语法：引入"
  },
  "TMS: wxs": {
    "prefix": "wxs",
    "body": [
      "<wxs src=\"./tools.wxs\" module=\"tools\" />"
    ],
    "description": "Reference: WXS 语法：模块"
  }
}