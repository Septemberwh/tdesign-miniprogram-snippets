{
  "name": "tdesign-miniprogram-snippets",
  "displayName": "tdesign-miniprogram-snippets",
  "description": "TDesign 微信小程序组件库的代码片段",
  "version": "0.0.3",
  "publisher": "SeptWong",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "tdesign",
    "miniprogram",
    "小程序",
    "wechat",
    "snippets"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "wxml",
        "extensions": [
          ".wxml"
        ]
      },
      {
        "id": "json",
        "extensions": [
          ".json"
        ]
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/wxml-snippets.json"
      },
      {
        "language": "wxml",
        "path": "./snippets/wxml-snippets.json"
      },
      {
        "language": "json",
        "path": "./snippets/json-snippets.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Septemberwh/tdesign-miniprogram-snippets"
  },
  "bugs": {
    "email": "septwong@foxmail.com"
  },
  "icon": "assets/images/logo.jpg",
  "license": "MIT License",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}